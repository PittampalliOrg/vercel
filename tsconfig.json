{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "sourceMap": true,
    "inlineSources": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "plugins": [
      {
        "name": "next"
      },
      {
        "transform": "typia/lib/transform",
        "functional": true
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/api-clients/*": [
        "./clients/dist/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "next.config.mjs"  ],
  "exclude": [
    "node_modules",
    "src",
    "clients",
    "/workspace/lib/typia",
    "/workspace/lib/generator.ts"
  ]
}