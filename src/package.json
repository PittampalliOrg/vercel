  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' NODE_OPTIONS='--enable-source-maps' NEXT_OTEL_VERBOSE=1 AUTH_SECRET=v0dZ03G4ECmMYMA4/Ht32mBeV1XR4znOTDffnyKxF5o= next dev --turbopack",
    "build": "next build -d",
    "start": "next start --keepAliveTimeout 70000",
    "info": "next info --verbose",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:gen-types": "supabase gen types typescript --db-url postgres://postgres:postgres@db:5432/postgres > database.types.ts",
    "typia": "pnpm run typia:codegen && pnpm run typia:generate",
    "typia:codegen": "node --experimental-strip-types ./lib/generator.ts",
    "typia:generate": "typia generate --input ./lib/typia --output ./lib/generated",
    "generate-dashboard": "jsonnet --jpath config/grizzly/vendor config/grizzly/main.jsonnet -o config/grafana/provisioning/dashboards/demo/output.json",
