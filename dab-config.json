{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v1.3.19/dab.draft.schema.json",
  "data-source": {
    "database-type": "postgresql",
    "connection-string": "Host=db;Port=5432;Database=postgres;User ID=postgres;Password=postgres;",
    "options": {}
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/api",
      "request-body-strict": true
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [
          "*"
        ],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "development"
    }
  },
  "entities": {
    "Suggestion": {
      "source": {
        "object": "public.suggestion",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Suggestion",
          "plural": "Suggestions"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Chat": {
      "source": {
        "object": "public.chat",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Chat",
          "plural": "Chats"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Document": {
      "source": {
        "object": "public.document",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Document",
          "plural": "Documents"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Message": {
      "source": {
        "object": "public.message",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Message",
          "plural": "Messages"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "User": {
      "source": {
        "object": "public.user",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "User",
          "plural": "Users"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Vote": {
      "source": {
        "object": "public.vote",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Vote",
          "plural": "Votes"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Unicorns": {
      "source": {
        "object": "public.unicorns",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Unicorns",
          "plural": "Unicorns"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    }
  }
}